<%= form_for @store do |f| %>
  <h4><%= f.label :name %></h4>
  <h4><%= f.text_field :name, value: @store.name %></h4>
  <h4><%= f.label :description %></h4>
  <h4><%= f.text_field :description, value: @store.description %></h4>
  <h4><%= f.label :image_url %></h4>
  <h4><%= f.text_field :image_url, value: @store.image_url %></h4>
  <p><%= f.submit "Submit", class: "btn btn-success btn-font-size" %></p>
<% end %>
