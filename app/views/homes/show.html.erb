<div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
    <li data-target="#carousel-example-generic" data-slide-to="1"></li>
    <li data-target="#carousel-example-generic" data-slide-to="2"></li>
  </ol>

  <!-- Wrapper for slides -->
  <div class="carousel-inner">
    <div class="item active">
      <img src="http://www.farmersmuseum.org/files/images/exhibitions/tfm-exhibitpage-carousel.jpg" alt="Smiley face" height="100" width="100">
      <div class="carousel-caption">
        <h2>Sign Up Now!</h2>
        <h4>Create an account to take advantage of wonderful home-grown goods!</h4>
      </div>
    </div>
    <div class="item cat-div">
      <img src="http://www.farmersmuseum.org/files/images/exhibitions/tfm-exhibitpage-carousel.jpg" alt="Smiley face" height="100" width="100">
      <div class="carousel-caption red-text">
        <h2>Freshest Products!</h2>
        <h4>Click here to see what's newest and most fresh on the shelves!</h4>
      </div>
    </div>
    <div class="item">
      <img src="http://www.farmersmuseum.org/files/images/exhibitions/tfm-exhibitpage-carousel.jpg" alt="Smiley face" height="100" width="100">
      <div class="carousel-caption">
        <h2>Featured Market</h2>
        <h4>See what out featured market has on special this week!</h4>
      </div>
    </div>
  </div>

  <!-- Controls -->
  <a class="left carousel-control" href="#carousel-example-generic" data-slide="prev">
    <span class="glyphicon glyphicon-chevron-left"></span>
  </a>
  <a class="right carousel-control" href="#carousel-example-generic" data-slide="next">
    <span class="glyphicon glyphicon-chevron-right"></span>
  </a>
</div>

